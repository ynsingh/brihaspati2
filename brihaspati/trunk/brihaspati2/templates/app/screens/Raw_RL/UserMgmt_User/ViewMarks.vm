<META HTTP-EQUIV="CACHE-CONTROL" CONTENT="NO-CACHE">
<META HTTP-EQUIV="PRAGMA" CONTENT="NO-CACHE">
$page.setBgColor($ui.bgcolor)
$page.setTitle("View Marks")

<table bgcolor="$ui.tableColor" width=100%>
<tr>
<td>
<font face="$ui.sansSerifFonts"><b>
<a href=$link.setPage("call,IndexHome.vm")>$brih_home</a> |
<a href=$link.setPage("call,CourseMgmt_User,CourseHome.vm")>$course</a> | $brih_view $brih_marks
</font>
</td>
</tr>
</table>
<br>
$!data.getMessage()
<form enctype="multipart/form-data" >
    <table align="center" bgcolor="$ui.bgcolor" cellpadding="0">
    #if($status != "Blank")	
      <tr>
	<td>
      	<b><u>$brih_marks $brih_in $course</u><b><br><br>
	</td>
      </tr>
      <tr>
      	<table border="1" cellpadding="0" cellspacing="1" align="center">
	<tr>
		#foreach($heading in $markHeading)
      			<th bgcolor="$ui.tableColor">&nbsp;&nbsp;$heading&nbsp;&nbsp;</th>
		#end
	</tr>
	<tr>	
	#foreach($entry in $marks)
		<td align="center">$entry</td>
	#end
	</tr>
      	</table>
      </tr>
    	#end
      <tr>
	#if($marksDSize == "0")
	<td colspan=3>
		$brih_MarksMsg2 '$data.getUser().getUserName()'!!
	</td>
	#end
	#if($Exp == "MExp")
	<td colspan=3>
		$brih_MarksMsg3
	</td>
	#end
	#if($Exp == "Exp")
	<td colspan=3>
		$brih_MarksMsg4
	</td>
	#end
      </tr>	
    </table>
</form>
