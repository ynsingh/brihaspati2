			<td>
        #elseif($tdcolor == "6")
           	<td bgcolor="#AFDCEC">
       	#end
        <b><I><a href=$link.setPage("call,OLES,OLES_ReEvaluation.vm").addPathInfo("count","6") style=text-decoration:none>$brih_reevaluation</I></b></a></td>
	</tr>
</table>
<table bgcolor="$ui.tableColor" width="100%">
	<tr bgcolor="$ui.menuColor" width="100%">
		<td align="left" width="100%" colspan="6"><b><i><font size="3">$brih_score $brih_reevaluation</font><b></i></td>
	</tr>
</table>

<table bgcolor="$ui.tableColor" width="100%">
	#if($collectScore.size()!=0)
	#if($flag=="true")
		<tr bgcolor="$ui.tableColor" width="100%">
			<th>QuizID</th>
			<th>StudentID(Student Name)</th>
			<th>Total Score</th>
		</tr>
	#end
	
       	#foreach($scr in $collectScore)
			#if($velocityCount%2==0)
				<tr bgcolor="$ui.menuColor">
			#else
				<tr bgcolor="$ui.tableColor">
			#end
			#if($scr.getEvaluate()=="ReEvaluate")
					<td>
						$scr.getQuizID()
					</td>
					<td>
						$scr.getUserID()($scr.getStudentName())
					</td>
					<td>
						$scr.getScore()
					</td>
					<td>	
						<b><I><a href=$link.setPage("call,OLES,Evaluate_Quiz.vm").addPathInfo("count","6").addPathInfo("quizID",$scr.getQuizID()).addPathInfo("type","ReEvaluate").addPathInfo("studentLoginName",$scr.getStudentName()).addPathInfo("actionName","eventSubmit_Evaluate") style=text-decoration:none>$brih_reevaluate</I></b></a>
					</td>
			#end
				</tr>
		#end
	#else
		<tr bgcolor="$ui.tableColor" width="100%">
			<td align="center" collspan="6">
			<b><font size="4">
				$!data.getMessage()
			</font></b>
			</td>
		</tr>
	#end
</table>		

